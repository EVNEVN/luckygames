setInterval(function () {
	if(run){
		var cur = new Date();
		var t = Math.floor((cur - startdate) / 1000);
		var hour = Math.floor(t / 3600);
		if (hour < 10) hour = '0' + Math.floor(t / 3600);
		t = t % 3600;
		var minutes = Math.floor(t / 60);
		if (minutes < 10) minutes = '0' + Math.floor(t / 60);
		var seconds = t % 60;
		if (seconds < 10) seconds = '0' + t % 60;
		('#_playtime').html(hour + ':' + minutes + ':' + seconds);
    }else{
		$('#_playtime').html('00:00:00');
	}	
},100);

function startstop(){
	run = !run;
	if(!run){
		//clearInterval(playinterval);
		$('#_start').text("START");
		$('#_start').css('color', 'green');
		$("#playBtn").removeClass("active");
	}else{
		$('#_start').text("STOP");
		$('#_start').css('color', 'red');
		//reset();
		startdate = new Date();
		balance = parseFloat(document.getElementById('balance').value);
		sbalance = balance;
		lastbalance = balance;
		basebetAmount = parseFloat($('#_BaseBet').val());
		overBalance = parseFloat($('#_OverBalance').val());
		underBalance = parseFloat($('#_UnderBalance').val());
		betAmount = basebetAmount;
		$('#betAmount').val(betAmount);
		prediction = Math.floor((Math.random() * (50 - 49 + 1)) + 49);
		$('#prediction').val(prediction);
		direction = 'under';
		$('#rollUnder').click();
		chance = prediction;
		bets = 0;
		    dps = [{
				x: 0,
				y: 0
			}];
		chart = new CanvasJS.Chart('chart', {
			theme: 'dark2',
			zoomEnabled: true,
			axisX: {
				title: 'Bets',
				includeZero: false,
			},
			axisY: {
				title: 'Profit',
				includeZero: false,
			},
			title: {
				fontColor: '#2eab5b',
				fontSize: 2e1,
				padding: 2e1
			},
			data: [{
				type: 'line',
				dataPoints: dps
			}]
		});
		chart.render();
		//playinterval = setInterval('PlayDice()',500);
		placeBet();
	//doBet();
		//playactive = 0;
	}
}

//=====================================
function placeBet() {
        if (1 == Game.playFlag) return !1;
        Game.playFlag = !0;
        var i = $("#betAmount").val();
        i = number_format(i, 8, ".", ""), $("#betAmount").val(i);
        var a = $("#clientSeed").val(),
            e = $("#serverSeedHash").html(),
            t = (i = $("#betAmount").val(), $("#prediction").val()),
            o = $("#coin").val();
        $("#playBtn").addClass("active"), jQuery.ajax({
            url: "/play/",
            type: "POST",
            dataType: "html",
            timeout: 6e4,
            data: {
                game: "dice",
                coin: o,
                betAmount: i,
                prediction: t,
                direction: Game.direction,
                clientSeed: a,
                serverSeedHash: e,
                hash: user.hash
            },
            success: function(l) {
                try {
                    l = JSON.parse(l);
			answer = l;
                } catch (a) {
                    return Game.playFlag = !1, 1 == Game.autoPlayFlag ? Game.abortAutoplay() : Game.stopAutoplay(), $.notification.error({
                        title: "Error",
                        message: user.locale.something_wrong
                    }), !1
                }
				betResult = l.gameResult;
                if (1 == l.result) Game.runDice(l.resultNumber, function() {
                    if ($("#serverSeedHash").html(l.serverSeedHash), $("#prevServerSeedHash").html(l.prevServerSeedHash), $("#prevServerSeed").html(l.prevServerSeed), $("#prevClientSeed").html(l.prevClientSeed), $("#balance").animateBalance(l.balance), $("#coin" + l.coinID + " span.balance").html(l.balance), updateStats(i, l.profit, l.gameResult), "on" == Game.sound) {
                        if ("lose" == l.gameResult) var a = new Audio("/assets/sounds/dice-lose.mp3");
                        if ("win" == l.gameResult) a = new Audio("/assets/sounds/dice-win.mp3");
                        a.play()
                    }
                    if ($("#myBets").hasClass("active") && !$("#bet" + l.id).length) {
                        var e = $("#listContainer .table .tbody .tr:first-child");
                        if (e.hasClass("full")) e.remove(), $("#listContainer .table .tbody").append(l.betHtml);
                        else {
                            e.before(l.betHtml);
                            var t = $("#listContainer .table .tbody .tr").length;
                            if (50 < t && (t = 40), 40 < t)
                                for (var o = 0; o < t - 40; o++) $("#listContainer .table .tbody .tr:last-child").remove()
                        }
                    }
					//=================================
		balance = answer.balance;
		profit = balance-sbalance;
		var prstr = profit.toFixed(8)+' ['+(profit*100/sbalance).toFixed(2)+'%]';
		$('#_status').text(prstr);
		if(profit<0)$('#_status').css('color','red');
		else $('#_status').css('color','green');
		bets++;
		updateChart(bets,profit,'gold');
		var fibonacci = (n, [p,c] = [0,basebetAmount]) => n == 0 ? 0 : n == 1 ? c : fibonacci(n-1, [c,p+c])

		if (overBalance != 0 && balance >= overBalance) {
			startstop();			
			alert('Over balance - Target Balance achieved')
			return;
		}
		if (underBalance != 0 && balance <= underBalance) {
			startstop();
			alert('Under balance - balance under Stop Lost Balance')
			return;
		}
		
		$('#clientSeed').val(generateClientSeed());

		if(answer.gameResult === 'lose'){
			round++;
			if (round % z === 0) {
			var rd = Math.floor((Math.random() * (2 - 1 + 1)) + 1);
			if (rd == 1) {
				prediction = Math.floor((Math.random() * (50 - 49 + 1)) + 49);
				direction = 'under';
				chance = prediction;
				$('#rollUnder').click();
			} else {
				prediction = Math.floor((Math.random() * (50 - 49 + 1)) + 49);
				direction = 'over';
				chance = 99 - prediction;
				$('#rollOver').click();
			}
			}
			//prediction = 95;
			$('#prediction').val(prediction);
			//direction = 'over';
			//$('#rollOver').click();
			//chance = 99-prediction;
			lossAmount = lastbalance - balance;
			//betAmount = lossAmount/(90/chance-1);
			
			n+=1;
			f = fibonacci(n);
			betAmount = f;
			if (betAmount < 0.00000001) betAmount = 0.00000001;

		}else{

			n-=1;
			if(n<1) n=1;
			f = fibonacci(n);
			betAmount = f;
			if (betAmount < 0.00000001) betAmount = 0.00000001;

			if(balance >= lastbalance){
				//$('#clientSeed').val(generateClientSeed());
				lastbalance = balance;
				var rdd = Math.floor((Math.random() * (2 - 1 + 1)) + 1);
				if (rdd == 1) {
				prediction = Math.floor((Math.random() * (50 - 49 + 1)) + 49);
				direction = 'under';
				chance = prediction;
				$('#rollUnder').click();
			} else {
				prediction = Math.floor((Math.random() * (50 - 49 + 1)) + 49);
				direction = 'over';
				chance = 99 - prediction;
				$('#rollOver').click();
				}
				//prediction = 98;
				//prediction = prediction1;	
				$('#prediction').val(prediction);
				//direction = direction1;
				if (direction == 'under') {
					$('#rollUnder').click();
				} else {
					$('#rollOver').click();
				}
				//$('#rollUnder').click();
				betAmount = basebetAmount;
				lossAmount = 0;
				n = 1;
				round = 0;
				z = Math.floor((Math.random() * (3 - 1 + 1)) + 1);
			}
		}
		$('#betAmount').val(betAmount.toFixed(8));	
		if(run) placeBet();									
					//=================================
                    
                    //$("#playBtn").removeClass("active")
                })
                else if (0 == l.result) {
                    if (Game.playFlag = !1, void 0 !== l.popupshow && $("#" + l.popupshow).popupShow(), void 0 !== l.serverHash) return $("#serverSeedHash").html(l.serverHash), Game.play(), !1;
                    Game.stopAutoplay(), void 0 !== l.msg && $.notification.error({
                        title: "Error",
                        message: l.msg
                    })
                }
            },
            error: function(a, e) {
                Game.playFlag = !1, 1 == Game.autoPlayFlag ? Game.abortAutoplay() : Game.stopAutoplay(), $.notification.error({
                    title: "Error",
                    message: user.locale.no_connection
                })
            },
			error: function(xhr, ajaxOptions, thrownError) {
				//alert('Fail, BOT will be stop!');
				randomizeSeed();
				placeBet();
			},
			timeout: function(xhr, ajaxOptions, thrownError) {
				randomizeSeed();
				placeBet();
			},
			abort: function(xhr, ajaxOptions, thrownError) {
				randomizeSeed();
				placeBet();
			}
        });

    }
//======================================	
	
function generateSeed(length) {
  let seed = '';
  const charset = 'abcdef0123456789';
  const array = new Uint32Array(length);
  window.crypto.getRandomValues(array);
  for (const val of array) {
    seed += charset.charAt(val % 11);
  }
  return seed;
}

function generateClientSeed() {
  return generateSeed(32);
}
